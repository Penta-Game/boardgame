{
    "name": "boardgame",
    "version": "1.0.0",
    "description": "boardgame.io implementation of pentagame",
    "productName": "boardgame",
    "main": "dist/index.js",
    "scripts": {
        "start": "tsc && electron-forge start",
        "watch": "tsc-watch --onFailure 'echo Beep!Compilation Failed'",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts .",
        "rebuild-linux": "./node_modules/.bin/electron-rebuild",
        "rebuild-windows": ".\\node_modules\\.bin\\electron-rebuild.cmd"
    },
    "keywords": [
        "boardgame.io",
        "pentagame"
    ],
    "author": {
        "name": "Cobalt",
        "email": "Chaosthe0rie@protonmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "_boardgame"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.51",
        "@electron-forge/maker-deb": "^6.0.0-beta.51",
        "@electron-forge/maker-rpm": "^6.0.0-beta.51",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
        "@electron-forge/maker-zip": "^6.0.0-beta.51",
        "@types/node": "^12.12.6",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "babel-eslint": "^10.1.0",
        "electron": "9.0.2",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.20.2",
        "tsc-watch": "^4.2.8",
        "typescript": "^3.5.2"
    },
    "dependencies": {
        "boardgame.io": "^0.39.14",
        "electron-squirrel-startup": "^1.0.0",
        "jquery": "^3.5.1",
        "module-alias": "^2.2.2",
        "node": "^14.4.0"
    },
    "bugs": {
        "url": "https://github.com/Penta-Game/boardgame/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Penta-Game/boardgame.git"
    },
    "_moduleAliases": {
        "@local": "dist/js",
        "@specific": "dist/specific"
    },
    "homepage": "https://github.com/Penta-Game/boardgame#readme"
}
